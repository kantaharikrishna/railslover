<div class="modal-header">
  <h3>Details</h3>
</div>
<div class="modal-body">
  <ul class="errors"></ul>
  <div><%= image_tag @memberimage.image_url.to_s, :width => 100 unless @memberimage.nil?  %></div>
  <div ><%= @detail.username.capitalize %></div>
  <div><%= @detail.email %></div>
</div>
<div class="modal-footer">
  <%= link_to "Got you,..Thanks", "#", class: "btn", data: { dismiss: "modal" } %>
</div>
